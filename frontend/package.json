{
  "name": "home-unite-us",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0 <21"
  },
  "scripts": {
    "dev": "vite",
    "dbg": "vite --debug",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cypress:run:mock": "cypress run --env USE_MOCK=true",
    "cypress:run:nomock": "cypress run --env USE_MOCK=false",
    "cypress:open": "cypress open --env USE_MOCK=true",
    "cypress:open:nomock": "cypress open --env USE_MOCK=false",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint . --ext ts --ext tsx --max-warnings 0 --fix",
    "type-check": "npx tsc --noEmit",
    "prepare": "cd .. && husky install frontend/.husky",
    "pre-commit": "lint-staged",
    "generate": "npx @rtk-query/codegen-openapi openapi-config.ts",
    "check-node-version": "node check-node-version.mjs",
    "preinstall": "npm run check-node-version"
  },
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*.@(ts|tsx|js|jsx)": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.6",
    "@mui/x-data-grid": "^6.15.0",
    "@mui/x-date-pickers": "^7.5.1",
    "@reduxjs/toolkit": "^1.8.3",
    "date-fns": "^2.30.0",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@faker-js/faker": "^8.1.0",
    "@rtk-query/codegen-openapi": "^1.0.0-alpha.1",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-loader": "^8.2.5",
    "cypress": "^13.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "msw": "^2.2.3",
    "prettier": "^3.2.5",
    "semver": "^7.6.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^5.4.8",
    "vitest": "^2.1.9"
  }
}
