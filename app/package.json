{
    "name": "home-unite-us",
    "version": "1.0.0",
    "description": "Home Unite Us: workflow management tool for empty bedrooms initiatives",
    "private": true,
    "proxy": "http://localhost:8080",
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "format": "prettier \"src/**/*.{js,html,ts,tsx}\" --write",
        "build": "webpack --config webpack.config.js",
        "build:docker": "webpack --config webpack-docker.config.js",
        "start": "webpack-dev-server --mode development --hot --open",
        "watch:flask": "webpack --config webpack-docker.config.js --watch --info-verbosity verbose",
        "dev": "webpack-dev-server --mode development --open --hot --config webpack-local.config.js",
        "build:prod": "webpack --mode production --progress --hide-modules",
        "check-type": "tsc -w"
    },
    "author": "Hack for LA",
    "license": "ISC",
